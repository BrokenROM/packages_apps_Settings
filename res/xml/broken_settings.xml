<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:settings="http://schemas.android.com/apk/res/com.android.settings"
        android:title="@string/broken_settings_title" >

        <PreferenceScreen
                android:key="status_bar"
                android:fragment="com.android.settings.broken.StatusBarSettings"
                android:title="@string/status_bar_title" />

        <PreferenceScreen
                android:key="navigation_bar"
                android:fragment="com.android.settings.broken.NavigationBarSettings"
                android:title="@string/navigation_bar_title" />

        <PreferenceScreen
                android:key="volume_keys_settings"
                android:title="@string/volume_keys_title"
                android:persistent="false"
                android:fragment="com.android.settings.broken.VolumeKeys" />

        <PreferenceScreen
                android:key="button_power_menu_settings"
                android:title="@string/button_power_menu_settings_title"
                android:summary="@string/button_power_menu_settings_summary"
                android:fragment="com.android.settings.broken.PowerMenuSettings" />

        <!-- Animations -->
        <PreferenceScreen
                android:id="@+id/animation_settings"
                android:title="@string/animation_settings_title"
                android:fragment="com.android.settings.broken.AnimationSettings" />

        <!-- Animations -->
        <PreferenceScreen
                android:id="@+id/animation_settings"
                android:title="@string/animation_settings_title"
                android:fragment="com.android.settings.broken.AnimationSettings" />

        <!-- App Circle -->
        <PreferenceScreen
                android:id="@+id/app_circle_sidebar"
                android:title="@string/app_circle_bar_title"
                android:summary="@string/app_circle_sidebar_summary_real"
                android:fragment="com.android.settings.broken.AppCircleSidebar" />

        <PreferenceScreen
				android:id="@+id/initD"
				android:fragment="com.android.settings.broken.InitD"
				android:title="@string/init_d_title"
				android:summary="@string/header_summary_init_d" />
				
		<PreferenceScreen
                android:key="hfm_settings"
                android:title="@string/hfm_pref_screen_title"
                android:fragment="com.android.settings.broken.hfm.HfmSettings" />


        <PreferenceScreen
                android:key="wakelock_blocker"
                android:fragment="com.android.settings.broken.WakeLockBlocker"
                android:title="@string/wakelock_blocker_title" />

        <!-- Screen Recorder -->
        <PreferenceScreen
                android:key="screen_recorder"
                android:title="@string/screen_recorder_title"
                android:fragment="com.android.settings.broken.ScreenRecorderSettings" />

</PreferenceScreen>
